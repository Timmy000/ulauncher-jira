{
  "manifest_version": "2",
  "api_version": "1",
  "name": "Jira Extension",
  "description": "Jira Search Extension",
  "developer_name": "Safa Ariman",
  "icon": "images/jira.png",
  "options": {
    "query_debounce": 1
  },
  "preferences": [
    {
      "id": "keyword",
      "type": "keyword",
      "name": "Jira Keyword",
      "default_value": "jira"
    },
    {
      "id": "url",
      "type": "input",
      "name": "Jira URL",
      "description": "Jira URL"
    },
    {
      "id": "username",
      "type": "input",
      "name": "Jira Username",
      "description": "Jira Username"
    },
    {
      "id": "password",
      "type": "input",
      "name": "Jira Password",
      "description": "Jira Password"
    }
  ]
}